{
    "name": "groovey/tester",
    "type": "project",
    "description": "The Code Incubator",         
    "keywords": [
        "php",
        "tester"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "monolog/monolog": "^2.2",
        "psr/log": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^5.3",        
        "symfony/http-foundation": "^5.2"        
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }        ,
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
